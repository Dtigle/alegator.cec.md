<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate"/>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
    <section name="quartz" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0,Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true" internalLogLevel="Debug" internalLogToConsole="true">
    <variable name="logDirectory" value="${basedir}/../Logs/${date:format=MM-yyyy}"/>
    <variable name="messageLayout" value="${longdate} ${uppercase:${level}} ${callsite} ${message} ${exception:format=tostring}"/>
    <targets async="true">
      <target name="errorfile" xsi:type="File" fileName="${logDirectory}/CEC.QuartzServer-${shortdate}.log" layout="${messageLayout}"/>
      <target name="statsfile" xsi:type="File" fileName="${logDirectory}/CEC.QuartzServer-${shortdate}-stats.log" layout="${messageLayout}"/>
      <target name="processingInfofile" xsi:type="File" fileName="${logDirectory}/CEC.QuartzServer-${shortdate}-processingInfo.log" layout="${messageLayout}"/>
      <target xsi:type="Console" name="c" layout="${messageLayout}"/>
    </targets>
    <rules>
      <logger name="NHibernate.SQL" writeTo="processingInfofile"/>
      <logger name="*" levels="Error" writeTo="errorfile"/>
      <logger name="*" levels="Warn,Info" writeTo="processingInfofile"/>
      <logger name="*" levels="Trace" writeTo="statsfile"/>
      <logger name="*" minlevel="Debug" writeTo="errorfile"/>
    </rules>
  </nlog>
  <connectionStrings>
    <add name="AmdarisConnectionString" connectionString="Data Source=192.168.1.142;Initial Catalog=CEC.SRV;Persist Security Info=True;User ID=sa;Password=Cecadmin2018" providerName="System.Data.SqlClient"/>
    <add name="SaiseConnectionString" connectionString="Data Source=192.168.1.142;Initial Catalog=SAISE.ElectionDay20190901;Persist Security Info=True;User ID=sa;Password=Cecadmin2018" providerName="System.Data.SqlClient"/>
  </connectionStrings>

  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory xmlns="urn:nhibernate-configuration-2.2" name="CEC.QuartzServer.Factory">
      <property name="connection.provider">NHibernate.Connection.DriverConnectionProvider</property>
      <property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>
      <property name="connection.driver_class">NHibernate.Driver.SqlClientDriver</property>
      <property name="show_sql">false</property>
      <property name="generate_statistics">false</property>
      <property name="connection.release_mode">auto</property>
      <property name="connection.isolation">ReadCommitted</property>
      <property name="connection.connection_string_name">AmdarisConnectionString</property>
      <property name="cache.provider_class">NHibernate.Cache.HashtableCacheProvider</property>
      <property name="cache.use_second_level_cache">true</property>
      <property name="cache.use_query_cache">true</property>
      <property name="adonet.batch_size">5000</property>
      <property name="command_timeout">3000</property>
    </session-factory>
  </hibernate-configuration>
  <quartz>
    <add key="quartz.checkConfiguration" value="false"/>
    <add key="quartz.scheduler.instanceName" value="ServerScheduler"/>
    <add key="quartz.scheduler.instanceId" value="SRVServerScheduler"/>

    <add key="quartz.threadPool.type" value="Quartz.Simpl.SimpleThreadPool, Quartz"/>
    <add key="quartz.threadPool.threadCount" value="10"/>
    <add key="quartz.threadPool.threadPriority" value="Normal"/>

    <!--<add key="quartz.jobStore.type" value="Quartz.Impl.AdoJobStore.JobStoreTX, Quartz"/>
  <add key="quartz.jobStore.misfireThreshold" value="60000"/>
  <add key="quartz.jobStore.dataSource" value="default"/>
  <add key="quartz.jobStore.driverDelegateType" value="Quartz.Impl.AdoJobStore.SqlServerDelegate, Quartz"/>
  <add key="quartz.jobStore.lockHandler.type" value="Quartz.Impl.AdoJobStore.UpdateLockRowSemaphore, Quartz"/>
  <add key="quartz.jobStore.tablePrefix" value="Quartz.QRTZ_"/>
  <add key="quartz.jobStore.useProperties" value="true"/>

  <add key="quartz.dataSource.default.connectionStringName" value="PQSConnectionString"/>
  <add key="quartz.dataSource.default.provider" value="SqlServer-20"/>-->

    <!--job initialization plugin handles our xml reading, without it defaults are used-->
    <add key="quartz.plugin.xml.type" value="Quartz.Plugin.Xml.XMLSchedulingDataProcessorPlugin, Quartz"/>
    <add key="quartz.plugin.xml.fileNames" value="~/quartz_jobs_SingleJob.xml"/>
    <add key="quartz.jobStore.type" value="Quartz.Simpl.RAMJobStore, Quartz"/>
    <add key="quartz.plugin.xml.scanInterval" value="20"/>

    <add key="quartz.server.type" value="CEC.QuartzServer.Core.CecQuartzServer, CEC.QuartzServer.Core"/>
    <add key="quartz.scheduler.exporter.type" value="Quartz.Simpl.RemotingSchedulerExporter, Quartz"/>
    <add key="quartz.scheduler.exporter.port" value="555"/>
    <add key="quartz.scheduler.exporter.bindName" value="QuartzScheduler"/>
    <add key="quartz.scheduler.exporter.channelType" value="tcp"/>
    <add key="quartz.scheduler.exporter.channelName" value="httpQuartz"/>
  </quartz>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <appSettings>


    <add key="SSRS_ReportExecutionService" value="http://192.168.1.142/ReportServer/ReportExecution2005.asmx"/>
    <add key="SSRS_UserName" value="Administrator"/>
    <add key="SSRS_Password" value="Cecadmin2018"/>
    <add key="SSRS_VoterReport" value="/SRV/VotersList_A3_2014"/>
    <add key="SSRS_AbroadVoterReport" value="/SRV/VotersList_Abroad_A3_2014"/>
    <add key="SSRS_LocalElectionsReport" value="/SRV/VotersList_LocalElections_A3_2015"/>
    <add key="SSRS_ResultsReport" value="/Parlamentare2014/ExternalWeb/ParliamentElectionStats_ExtWeb"/>
    <add key="SSRS_TurnoutReportsPath" value="/Parlamentare2014/ExternalWeb/"/>
    <add key="ListPrintingJob_ExportPath" value="c:\temp\"/>
    <add key="ResultsReportsPrintOutJob_ExportPath" value="c:\temp\"/>
    <add key="SSRS_ExportFormat" value="MHT"/>
    <add key="ResultsReports_ExportFormats" value="MHT,PDF"/>

    <add key="RspUri" value="http://XXXXXX:8080/cec/services/Cec"/>
    <add key="RspClassifierUri" value="http://XXXXXX:8080/classifier/services/Classifier"/>
    <add key="RspUser" value="cecuser"/>
    <add key="RspPass" value="cecuser"/>
    <add key="RspModificationProcessorJob_IgnoreNewEntries" value="false"/>
    <add key="UpdateClassifiersEnabled" value="false"/>
    <add key="saiseTargetElectionId" value="52"/>
    <add key="TargetElectionDate" value="2014-11-30"/>
    <add key="SaiseExporterJob_IgnoreMissingSAISEIdinSRVRegion" value="true"/>
    <add key="SaiseExporterJob_IgnorePeopleWithoutDoc" value="true"/>
    <add key="IgnorableDistrictsNumbers" value=""/>

    <add key="ResultsReportsPrintOutJob_ReportPageHeight" value="44cm"/>
    <add key="ResultsReportsPrintOutJob_ShowMandates" value="false"/>

    <add key="TurnoutFixationJob_turnoutControlTimes" value="Open;09:30;12:30;15:30;18:30;21:30"/>
    <add key="TurnoutReportsPrintOutJob_ReportPageHeight" value="50cm"/>
    <add key="TurnoutFixationJob_TimeOfEntry" value="Open"/>

  </appSettings>

  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="rspBinding" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" allowCookies="false" bypassProxyOnLocal="false" hostNameComparisonMode="StrongWildcard" maxBufferPoolSize="524288" maxBufferSize="9000000" maxReceivedMessageSize="9000000" textEncoding="utf-8" transferMode="Buffered" useDefaultWebProxy="true" messageEncoding="Text">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <security mode="TransportCredentialOnly">
            <transport clientCredentialType="Basic" proxyCredentialType="None" realm=""/>
            <message clientCredentialType="UserName" algorithmSuite="Default"/>
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://XXXXXXX:8080/classifier/services/Classifier" binding="basicHttpBinding" bindingConfiguration="rspBinding" contract="RspClassifierService.Classifier" name="Classifier"/>
      <endpoint address="http://XXXXXXXXXX:8080/password/services/password" binding="basicHttpBinding" bindingConfiguration="rspBinding" contract="RspPasswordService.Password" name="password"/>
      <endpoint address="http://XXXXXXXXX:8080/cec/services/Cec" binding="basicHttpBinding" bindingConfiguration="rspBinding" contract="RspCecService.Cec" name="Cec"/>
    </client>
  </system.serviceModel>

  <system.net>
    <settings>
      <servicePointManager expect100Continue="false"/>
    </settings>
  </system.net>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NLog" publicKeyToken="5120e14c03d0593c" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-3.2.0.0" newVersion="3.2.0.0"/>
      </dependentAssembly>

      <dependentAssembly>
        <assemblyIdentity name="Common.Logging" publicKeyToken="af08829b84f0328e" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0"/>
      </dependentAssembly>

      <dependentAssembly>
        <assemblyIdentity name="Quartz" publicKeyToken="f6b8c98a402cc8a4" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2.3.1.0" newVersion="2.3.1.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

</configuration>
