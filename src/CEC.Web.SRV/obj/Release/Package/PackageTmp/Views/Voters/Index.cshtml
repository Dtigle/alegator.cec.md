@using CEC.Web.SRV.Resources

@{
	ViewBag.Title = MUI.Voters_TabTitle;
}

<div class="row">
	<div class="col-xs-12">
		<div class="box">
			<div class="box-content">
				<h5 class="page-header">@MUI.Voters_SearchLabel</h5>
				@Html.Partial("_SearchIdnp")
			</div>
		</div>
		<div class="box">
			<div class="box-content">
				<h5 class="page-header">@MUI.VotersMgmt</h5>
				@Html.Partial("_FilterForVoters")
				@Html.Partial("_VotersList")
			</div>
		</div>
	</div>
</div>

<script type="text/javascript">
	$.getRegionId = function () {
		return { regionId: $('#RegionId').val() };
	};
</script>