@using CEC.Web.SRV.Infrastructure.Grids
@using CEC.Web.SRV.Models.History
@using CEC.Web.SRV.Resources
@using Lib.Web.Mvc.JQuery.JqGrid
@model long

@{
    var grid = Html.GridOptions<RegionTypeHistoryGridModel>("regionTypeHistory", width: 800)
        .BuildGrid(Url.Action("ListRegionTypeHistoryAjax", new {id = Model}),showFilterToolbar:false,columnChooser:false)
		.Navigator(new JqGridNavigatorOptions { Add = false, Edit = false, Delete = false, Search = false });
}

<div class="clearfix">
    @grid.GetHtml()
</div>

<script type="text/javascript">
    $(document).ready(function () {
        @grid.GetJavaScript();
        $('#regionTypeHistory').jqGrid().remapColumns([1, 2, 0, 3, 4, 5, 6, 7, 8], true, false);
        $('.modal-content').attr('style', 'width:840px;');
    });
</script>