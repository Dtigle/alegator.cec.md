    <DataSet Name="dsCircumscriptions">
      <Query>
        <DataSourceName>AdminDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ElectionRoundId">
            <Value>=Parameters!ElectionRoundId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
*
FROM
(SELECT 
       [electionCircumscriptionId]
      ,[electionRoundId]
      ,[number]
      ,[nameRo]
FROM [SAISE].[ElectionCircumscriptions]
WHERE   electionRoundId = @ElectionRoundId 
UNION ALL
SELECT
0,
0,
'0',
N'- Toate circumscrip»õiile -') as circ
ORDER BY CAST(N'/' + LTrim(RTrim(circ.number)) + N'/' AS HIERARCHYID)</CommandText>
      </Query>
