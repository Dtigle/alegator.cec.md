            <tr>
                <td class="letter-cell">
                    <span class="badge badge-letter">a)</span>
                    numărul depus până în ziua votării

                    <div data-bind="visible: !ContestationsSubmittedToCouncil.isValid() && !ContestationsSubmittedToCouncil_Rule_Succeeded() && ShowValidationErrors()">
                        <div class="alert alert-danger alert-letter">
                            <span>Regula: </span>
                            <code>a = 5(a) + 5(e) + 5(f) + 5(g)</code>
                        </div>
                    </div>
                </td>

                <td data-bind="validationElement: ContestationsSubmittedToCouncil">
                    <input data-bind="value: ContestationsSubmittedToCouncil" class="form-control number" maxlength="10" />
                </td>
            </tr>

            <tr>
                <td class="letter-cell">
                    <span class="badge badge-letter">b)</span>
