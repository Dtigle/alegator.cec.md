            <tr>
                <td>
                    <span class="badge">1.</span>
                    Componența numerică a membrilor consiliului electoral

                    <div data-bind="visible: CouncilMembers() > 11 && ShowValidationErrors()">
                        <div class="alert alert-danger">
                            <span>maximum 11</span>
                        </div>
                    </div>
                </td>

                <td data-bind="validationElement: CouncilMembers, css: CouncilMembers() > 11 ? 'has-error' : ''">
                    <input data-bind="value: CouncilMembers" class="form-control number" maxlength="10" />
                </td>
            </tr>

            <tr>
                <td>
                    <span class="badge">2.</span>
                    Numărul ședințelor organizate pe parcursul desfășurării activității
                </td>

                <td>
                    <input data-bind="value: MeetingsDuringElectionPeriod" class="form-control number" maxlength="10" />
                </td>
            </tr>

            <tr>
                <td>
                    <span class="badge">3.</span>
                    Numărul hotărârilor adoptate de către consiliul electoral
                </td>

                <td>
                    <input data-bind="value: DecisionsDuringElectionPeriod" class="form-control number" maxlength="10" />
                </td>
            </tr>

            <tr>
                <td>
                    <span class="badge">4.</span>
